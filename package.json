{
  "name": "concannotator",
  "version": "0.0.9",
  "description": "Desktop application for annotation of concordances.",
  "main": "main.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/montesmariana/concAnnotator.git"
  },
  "keywords": [
    "linguistics",
    "semmantics",
    "annotation"
  ],
  "author": "Mariana Montes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/montesmariana/concAnnotator/issues"
  },
  "homepage": "https://github.com/montesmariana/concAnnotator#readme",
  "devDependencies": {
    "electron": "^21.2.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "concannotator"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
